--echo #
--echo # Testing range operations for varying data types.
--echo #

--echo #
--echo # Date type YEAR
--echo #
CREATE TABLE t1 (pk YEAR PRIMARY KEY);
INSERT INTO t1 VALUES (2020), (2021), (2022), (2023), (2024);
SELECT COUNT(*) FROM t1 WHERE pk > 2025;
SELECT pk FROM t1 WHERE pk > 2025;
SELECT COUNT(*) FROM t1 WHERE pk < 2030;
SELECT pk FROM t1 WHERE pk < 2030;
SELECT pk FROM t1 WHERE pk = 2025;
SELECT pk FROM t1 WHERE pk = 2022;
SELECT pk FROM t1 WHERE pk <> 2022;
SELECT pk FROM t1 WHERE pk BETWEEN 2021 AND 2023;
SELECT pk FROM t1 WHERE pk IN ('2025', '2026');
SELECT pk FROM t1 WHERE pk IN ('2021', '2022');
SELECT pk FROM t1 WHERE pk NOT IN ('2025', '2026');
SELECT pk FROM t1 WHERE pk NOT IN ('2021', '2022');
DROP TABLE t1;
