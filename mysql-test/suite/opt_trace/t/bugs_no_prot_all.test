# Start server with MyISAM
--source include/force_myisam_default.inc

# Skip the test if MyISAM is not available
--source include/have_myisam.inc

# Does not output the same optimizer trace.
--source include/not_hypergraph.inc

set optimizer_switch='semijoin=on,materialization=on,firstmatch=on,loosescan=on,index_condition_pushdown=on,mrr=on,mrr_cost_based=off';

# Test requires: sp-protocol/ps-protocol/view-protocol/cursor-protocol disabled
--source include/no_protocol.inc

# This test doesn't support GTID
--source include/rpl/set_gtid_mode_off.inc

--let $support_native_partitioning= 0
--source suite/opt_trace/include/bugs.inc

# Restore the default for gtid_mode which is ON
--source include/rpl/set_gtid_mode_on.inc
